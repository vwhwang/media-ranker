<h1>Users page</h1>
<table>
  <tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username,user_path(id: user.id) %></td>
      <td>0</td>
      <td><%=user.created_at.strftime('%B %d, %Y') %></td>
    </tr>
  <% end  %>
</table>

<%= link_to "Back to Media List", root_path %>